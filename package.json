{
  "name": "@aarnphm/morph-monorepo",
  "private": true,
  "homepage": "https://tinymorph.aarnphm.xyz",
  "scripts": {
    "morph:docs": "pnpm --filter ./docs run quartz build --serve -v",
    "morph:docs:build": "pnpm --filter ./docs run quartz build -v",
    "morph:inference": "cd ./python/asteraceae/ && make stack",
    "morph:inference:build": "cd ./python/asteraceae/ && make build",
    "morph:dev": "pnpm --filter morph run dev",
    "morph:build": "pnpm --filter morph run build",
    "morph:format": "pnpm --filter morph run format && uvx ruff format"
  },
  "keywords": [
    "sae",
    "bentoml",
    "mech interp"
  ],
  "author": "Aaron Pham <contact@aarnphm.xyz>",
